{
  "name": "quasar.template.app",
  "version": "0.0.0",
  "description": "A Quasar Project",
  "productName": "Quasar Template",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev:pwa": "quasar dev -m pwa -p 8080 --dev",
    "dev:spa": "quasar dev -m spa -p 8081 --dev",
    "build:dev:pwa": "quasar build -m pwa --debug --dev",
    "build:dev:spa": "quasar build -m spa --debug --dev",
    "build:pre:pwa": "quasar build -m pwa --pre",
    "build:pre:spa": "quasar build -m spa --pre",
    "build:prod:pwa": "quasar build -m pwa --prod",
    "build:prod:spa": "quasar build -m spa --prod",
    "preview:dev:pwa": "npm run build:dev:pwa && vite preview --outDir build/dev --port 8080 --open",
    "preview:dev:spa": "npm run build:dev:spa && vite preview --outDir build/dev --port 8081 --open",
    "preview:pre:pwa": "npm run build:pre:pwa && vite preview --outDir build/pre --port 8080 --open",
    "preview:pre:spa": "npm run build:pre:spa && vite preview --outDir build/pre --port 8081 --open",
    "preview:prod:pwa": "npm run build:prod:pwa && vite preview --outDir build/prod --port 8080 --open",
    "preview:prod:spa": "npm run build:prod:spa && vite preview --outDir build/prod --port 8081 --open",
    "deploy-patch": "npm version patch -m v%s",
    "deploy-minor": "npm version minor -m v%s",
    "deploy-major": "npm version major -m v%s",
    "preversion": "npm run lint && npm run sonar && npm run trivy",
    "version": "git add -A",
    "postversion": "git push origin main && git push origin --tags",
    "sonar": "powershell -ExecutionPolicy Bypass -File ./sonar.ps1",
    "trivy": "npm ci && powershell -ExecutionPolicy Bypass -File ./trivy.ps1",
    "prepare": "quasar prepare",
    "postinstall": "quasar prepare && npm audit fix"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "pinia": "^3.0.1",
    "quasar": "^2.18.5",
    "register-service-worker": "^1.7.2",
    "vue": "^3.4.18",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.14",
    "prettier": "^3.3.3",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-compression": "^0.5.1",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
